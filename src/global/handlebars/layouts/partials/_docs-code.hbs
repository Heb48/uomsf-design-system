<div class="uom-docs-code">
{{#unless open}}
  <button class="uom-link uom-link--button uom-link--icon uom-small js-code-button">
    <span>Show code</span>
    <span class="material-icons uom-material-icons" focusable="false" aria-hidden="true">keyboard_arrow_down</span>
  </button>
{{/unless}}
  <div class="uom-docs-code__content{{#if open}} active{{/if}}">
    <button type="button" class="uom-button uom-button--dark-outline-solid uom-button--flex uom-button--small js-code-copy">
      <span>Copy</span>
      <span class="material-icons uom-material-icons" focusable="false" aria-hidden="true">content_copy</span>
    </button>
    <div class="uom-docs-code__copy" focusable="false" aria-hidden="true" tabindex="-1"><script>document.write((`{{> @partial-block }}`).replace(/</g, "&lt;").replace(/>/g, "&gt;"));</script></div>
    <div class="uom-docs-code__wrapper">
<pre><code class="{{#if code}}{{code}}{{else}}html{{/if}}"><script>document.write((`{{> @partial-block }}`).replace(/</g, "&lt;").replace(/>/g, "&gt;").replace(/"/g, "&quot;").replace(/'/g, "&#039;"));</script></code></pre>
    </div>
  </div>
</div>
