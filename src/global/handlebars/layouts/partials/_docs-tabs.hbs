<div class="uom-docs__tabs">
  <div class="uom-tabs js-tabs uom-docs-tabs">
    <ul class="uom-tabs__list">
      <li class="uom-tabs__list-item">
        <a href="#section-guidance" class="uom-tabs__link js-tabs-fixed">Guidance</a>
      </li>
      <li class="uom-tabs__list-item">
        <a href="#section-interactive-demo" class="uom-tabs__link js-tabs-fixed">Demo &amp; Code</a>
      </li>
    </ul>
    <section id="section-guidance" class="uom-tabs__content">
      <div class="uom-docs__container">
      {{#each (lookup guidancetab directory)}}
        {{{contents}}}
      {{/each}}
      {{#each (lookup templatesnav directory)}}
        {{{item}}}
      {{/each}}
      </div>
    </section>
    <section id="section-interactive-demo" class="uom-tabs__content">
      <div class="uom-docs__demo">
        {{#unless no-blank}}
        <div class="uom-docs__external">
          <div class="uom-list uom-list--8">
            <a class="uom-small uom-link uom-link--icon" target="_blank" href="{{change-extention path 'blank'}}"><span>View in new window</span><i class="material-icons uom-material-icons" focusable="false" aria-hidden="true">launch</i></a>{{#if template}}<a class="uom-small uom-link uom-link--icon" target="_blank" href="{{template}}"><span>View in template</span><i class="material-icons uom-material-icons" focusable="false" aria-hidden="true">launch</i></a>{{/if}}{{#if theme}}<a class="uom-small uom-link uom-link--icon" target="_blank" href="{{change-extention path 'theme'}}"><span>View theming example</span><i class="material-icons uom-material-icons" focusable="false" aria-hidden="true">launch</i></a>{{/if}}
          </div>
        </div>
        {{/unless}}
{{{contents}}}
      </div>
    </section>
  </div>
</div>
