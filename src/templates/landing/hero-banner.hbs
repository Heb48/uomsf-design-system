order: 16
page: true
model:
  nav: ../../components/main-nav/json/main-nav-digital-nsw.json
  breadcrumbs: ../../components/breadcrumbs/json/breadcrumbs-landing.json
  footer: ../../components/footer/json/footer-landing.json
---


{{>_masthead content="#content" nav="#main-nav"}}
{{>_header header-title="digital.nsw"}}
{{>_main-nav model.nav}}

<div class="uom-container uom-p-bottom-sm">
  {{>_breadcrumbs model.breadcrumbs}}
</div>

{{>_hero-banner
  wide=true
  lines=true
  theme="dark"
  heading="Beyond Digital"
  copy="Beyond Digital sets the strategic directions and objectives for NSW to continue its exciting journey in becoming a leading customer-centric, digitally-enabled government."
}}

<main id="content">

  <section class="uom-section">
    <div class="uom-container">
      <div class="uom-layout">
        <div class="uom-layout__main">
          <h2 class="uom-section-title">Explore the strategy</h2>
          <div class="uom-grid">
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
            {{>_card
              headline=true
              heading="Strategy in action"
            }}
            </div>
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
            {{>_card
              headline=true
              heading="NSW Connectivity strategy"
            }}
            </div>
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
            {{>_card
              headline=true
              heading="Our customer commitments"
            }}
            </div>
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
            {{>_card
              headline=true
              heading="Strategy directions"
            }}
            </div>
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
            {{>_card
              headline=true
              heading="Capabilities for the future"
            }}
            </div>
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
            {{>_card
              headline=true
              heading="The minister’s involvement"
            }}
            </div>
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
            {{>_card
              headline=true
              heading="Use of data insights"
            }}
            </div>
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
            {{>_card
              headline=true
              heading="What’s new"
            }}
            </div>
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
            {{>_card
              headline=true
              heading="Summary of the strategy"
            }}
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <section class="uom-section uom-section--brand-light">
    <div class="uom-container">
      <div class="uom-layout">
        <div class="uom-layout__main">

          <div class="uom-grid uom-align-items-center uom-justify-content-center">
            <div class="uom-col uom-col-md-6">
              {{>_media
                image="https://picsum.photos/id/838/2000/1050"
                title="A picture of a black dog"
              }}
            </div>
            <div class="uom-col uom-col-md-6">
              <h2>UOM Data Strategy</h2>
              <p class="uom-m-bottom-sm">The UOM Data Strategy sets a collaborative, coordinated, consistent and safe approach to using and sharing data and insights across government to inform decisions and actions to achieve the best possible outcomes for the people and businesses of NSW.</p>
              {{>_button-link url="#" style="dark" text="UOM Data Strategy"}}
            </div>
          </div>
          <div class="uom-grid uom-align-items-center uom-justify-content-center">
            <div class="uom-col uom-col-md-6 uom-order-lg-last">
              {{>_media
                image="https://picsum.photos/id/524/2000/1050"
                title="A picture of a black dog"
              }}
            </div>
            <div class="uom-col uom-col-md-6 uom-order-lg-first">
              <h2>NSW Connectivity Strategy</h2>
              <p class="uom-m-bottom-sm">NSW Telco Authority has developed the NSW Connectivity Strategy to align and optimise state-wide programs and opportunities to ensure modern, high speed digital networks are available to all.</p>
              {{>_button-link url="#" style="dark" text="NSW Connectivity Strategy"}}
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <section class="uom-section">
    <div class="uom-container">
      <div class="uom-layout">
        <div class="uom-layout__main">
          <h2 class="uom-section-title">Related initiatives</h2>
          <div class="uom-grid">
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
              {{>_card
              headline=true
              theme="dark"
              heading="Bringing your digital identity to life"
              copy="We’re working hard to deliver a convenient, secure, and private digital identity solution for our customers across NSW."
              }}
            </div>
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
              {{>_card
              headline=true
              theme="dark"
              heading="Helping students plan and implement employment and education pathways"
              copy="With the NSW Education Wallet students will plan and implement employment and education pathways that suit them."
              }}
            </div>
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
              {{>_card
              headline=true
              theme="dark"
              heading="Digital solutions improve water licensing processes"
              copy="The water licensing improvement program will simplify processes, policies and ICT systems to deliver a better experience for NSW water users."
              }}
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <section class="uom-section uom-section--off-white">
    <div class="uom-container">
      <div class="uom-layout">
        <div class="uom-layout__main">
          <h2 class="uom-section-title">Stay in the loop and sign up to the digital.nsw newsletter</h2>
          <p class="uom-section-text">Find out about work happening across UOM in the digital space.</p>
          <div class="uom-grid uom-align-items-center uom-justify-content-center">
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
              {{>_text-input id="form-first-name" label="First Name" required=true }}
            </div>
            <div class="uom-col uom-col-md-6 uom-col-lg-4">
              {{>_text-input id="form-last-name" label="Last Name" required=true }}
            </div>

            <div class="uom-col uom-col-md-6 uom-col-lg-4">
              {{>_text-input id="form-email" label="Email" required=true }}
            </div>
          </div>
          <div class="uom-grid">
            <div class="uom-col uom-col-md-6 uom-col-lg-4 uom-p-top-0">
              {{>_button type="button" style="dark" text="Submit"}}
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
</main>

{{>_footer model.footer}}
