<div class="uom-tabs js-tabs{{#if fixed}} js-tabs-fixed{{/if}}">
  <ul class="uom-tabs__list">
    {{#each tabitems}}
    <li><a href="#{{urlHash}}">{{title}}</a></li>
    {{/each}}
  </ul>
  {{#each tabitems}}
  <section id="{{urlHash}}" class="uom-tabs__content">
    {{{content}}}
  </section>
  {{/each}}
</div>
